{
  "compilerOptions": {
    "target": "esnext", // 代码可使用的语法版本
    "module": "commonjs", // ts编译为js的模块规定
    "outDir": "dist", // 编译输出的文件夹
    "rootDir": "src", // 编译的文件夹
    "jsx": "react-jsx", // 支持react-jsx
    "noImplicitAny": true, // 推断的any报错提示
    "esModuleInterop": true // esm支持直接导入cjs
  }
}
